@{
    ViewData["Title"] = "Home Page";
}

<div class="row m-5">
    <div class="col">
        <h2>
            Random Password Generator
        </h2>
    </div>
</div>
<div class="row m-5">
    <div class="col-10">
        <input class="form-control" type="text" value="@ViewBag.Password">
        <span class="text-secondary">Password #@ViewBag.cnt</span>
    </div>
    <div class="col-2">
        <a class="btn btn-block btn-outline-primary" href="/">Generate</a>
    </div>
</div>
